{
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": 0,
    "no-empty": 0,
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "never"],
    "object-curly-spacing": [
      "error",
      "always",
      { "arraysInObjects": true, "objectsInObjects": true }
    ],
    "no-invalid-this": "off",
    "new-cap": "off"
  }
}
